{
    "editor.unicodeHighlight.nonBasicASCII": false,
    "workbench.sideBar.location": "right",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "git.autofetch": true,
    "workbench.startupEditor": "none",
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": true,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "repositories": false,
        "searchAndCompare": true,
        "launchpad": false
    },
    "kotlin.languageServer.enabled": false,
    "kotlin.debugAdapter.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.tabSize": 2,
    "files.autoGuessEncoding": true,
    "FSharp.indentationSize": 2,
    "artisan.docker.command": "",
    "liveServer.settings.donotShowInfoMsg": true,
    "files.trimTrailingWhitespace": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.extension.enableComposeLanguageServer": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "Noctis Minimus",
    "editor.renderWhitespace": "all",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "python": false
    },
    "workbench.colorCustomizations": {
        "editorIndentGuide.background1": "#b22222",
        "editorIndentGuide.background2": "#ff4500",
        "editorIndentGuide.background3": "#ffd700",
        "editorIndentGuide.background4": "#228b22",
        "editorIndentGuide.background5": "#007380",
        "editorIndentGuide.background6": "#8b008b",
        "editorIndentGuide.activeBackground1": "#fff",
        "editorIndentGuide.activeBackground2": "#fff",
        "editorIndentGuide.activeBackground3": "#fff",
        "editorIndentGuide.activeBackground4": "#fff",
        "editorIndentGuide.activeBackground5": "#fff",
        "editorIndentGuide.activeBackground6": "#fff"
    }
}