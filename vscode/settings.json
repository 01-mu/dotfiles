{
    "editor.unicodeHighlight.nonBasicASCII": false,
    "workbench.sideBar.location": "right",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "git.autofetch": true,
    "workbench.startupEditor": "none",
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": true,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "repositories": false,
        "searchAndCompare": true,
        "launchpad": false
    },
    "kotlin.languageServer.enabled": false,
    "kotlin.debugAdapter.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.tabSize": 2,
    "files.autoGuessEncoding": true,
    "FSharp.indentationSize": 2,
    "artisan.docker.command": "",
    "liveServer.settings.donotShowInfoMsg": true,
    "files.trimTrailingWhitespace": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.extension.enableComposeLanguageServer": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "Noctis Azureus",
    "editor.renderWhitespace": "all",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "python": false
    },
    "indentRainbow.includedLanguages": [
        "html",
        "python"
    ],
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.colors": [
        "rgba(128,  64,  64, 1)",
        "rgba(128,  96,  32, 1)",
        "rgba(128, 128,  32, 1)",
        "rgba( 32, 128,  32, 1)",
        "rgba( 32,  64, 128, 1)",
        "rgba(128,  64, 128, 1)"
    ],
    "indentRainbow.lightIndicatorStyleLineWidth": 1.5,
}